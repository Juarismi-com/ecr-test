{
  "family": "ecr-test-nodejs-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::423623862620:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::423623862620:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "ecr-test",
      "image": "423623862620.dkr.ecr.us-east-1.amazonaws.com/ecr-test:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecr-test-nodejs-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}

